  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Launch node.js backup command",
      "program": "${workspaceFolder}/src/index.js",
      "args": ["backup", "bundles"]
    },
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Launch node.js elasticsearch command",
      "program": "${workspaceFolder}/src/index.js",
      "args": ["load-index", "bundles"]
    },
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Launch node.js firestore command",
      "program": "${workspaceFolder}/src/index.js",
      "args": ["firestore-docs", "-p", "bundles/f0a0e61d-0c7b-4cd8-8b5f-e2ac32e0bef3"]
    },
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/src"
    }
  ]
}